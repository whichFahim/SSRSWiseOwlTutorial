  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>dscMovies</DataSourceName>
        <CommandText>SELECT tblDirector.DirectorName, tblFilm.FilmName, tblFilm.FilmRunTimeMinutes, tblCountry.CountryName
FROM   tblFilm INNER JOIN
             tblDirector ON tblFilm.FilmDirectorID = tblDirector.DirectorID INNER JOIN
             tblCountry ON tblFilm.FilmCountryID = tblCountry.CountryID</CommandText>
      </Query>
