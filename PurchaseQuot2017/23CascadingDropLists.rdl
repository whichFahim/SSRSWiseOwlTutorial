          <Operator>Equal</Operator>
          <FilterValues>
            <FilterValue>=Parameters!DirectorID.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
    <DataSet Name="dtsActorRoles">
      <Query>
        <DataSourceName>dscMovies</DataSourceName>
        <CommandText>SELECT tblActor.ActorName, tblCast.CastCharacterName, tblCast.CastFilmID
