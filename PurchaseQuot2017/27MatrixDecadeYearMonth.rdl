  <DataSets>
    <DataSet Name="dtsFilmDates">
      <Query>
        <DataSourceName>dscMovies</DataSourceName>
        <CommandText>SELECT tblFilm.FilmName, tblFilm.FilmReleaseDate, tblCountry.CountryName
FROM   tblFilm INNER JOIN
             tblCountry ON tblFilm.FilmCountryID = tblCountry.CountryID</CommandText>
      </Query>
      <Fields>
        <Field Name="FilmName">
          <DataField>FilmName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilmReleaseDate">
          <DataField>FilmReleaseDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="CountryName">
          <DataField>CountryName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilmDecade">
          <Value>=Switch(Year(Fields!FilmReleaseDate.Value)&lt;2010 AND Year(Fields!FilmReleaseDate.Value)&gt;=2000, "2000s",
Year(Fields!FilmReleaseDate.Value)&lt;2000 AND Year(Fields!FilmReleaseDate.Value)&gt;=1990, "1990s",
Year(Fields!FilmReleaseDate.Value)&lt;1990 AND Year(Fields!FilmReleaseDate.Value)&gt;=1980, "1980s",
Year(Fields!FilmReleaseDate.Value)&lt;1980 AND Year(Fields!FilmReleaseDate.Value)&gt;=1970, "1970s",
Year(Fields!FilmReleaseDate.Value)&lt;1970 AND Year(Fields!FilmReleaseDate.Value)&gt;=1960, "1960s",
Year(Fields!FilmReleaseDate.Value)&lt;1960 AND Year(Fields!FilmReleaseDate.Value)&gt;=1950, "1950s",
Year(Fields!FilmReleaseDate.Value)&lt;1950 AND Year(Fields!FilmReleaseDate.Value)&gt;=1940, "1940s",
Year(Fields!FilmReleaseDate.Value)&lt;1940 AND Year(Fields!FilmReleaseDate.Value)&gt;=1930, "1930s")</Value>
        </Field>
        <Field Name="FilmYear">
          <Value>=Year(Fields!FilmReleaseDate.Value)</Value>
        </Field>
        <Field Name="FilmMonth">
          <Value>=MonthName(Month(Fields!FilmReleaseDate.Value))</Value>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
