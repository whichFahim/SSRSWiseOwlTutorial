  <DataSets>
    <DataSet Name="dtsFilmDates">
      <Query>
        <DataSourceName>dscMovies</DataSourceName>
        <CommandText>SELECT FilmName, FilmReleaseDate
FROM   tblFilm</CommandText>
      </Query>
      <Fields>
        <Field Name="FilmName">
          <DataField>FilmName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilmReleaseDate">
          <DataField>FilmReleaseDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="FilmMonth">
          <Value>=MonthName(Month(Fields!FilmReleaseDate.Value))</Value>
        </Field>
        <Field Name="FilmDay">
          <Value>=WeekdayName(Weekday(Fields!FilmReleaseDate.Value))</Value>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!FilmDay.Value</FilterExpression>
          <Operator>Equal</Operator>
          <FilterValues>
            <FilterValue>=Parameters!Weekday.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
